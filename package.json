{
  "name": "solana-trade",
  "version": "1.0.10",
  "description": "A comprehensive Solana trading library supporting multiple DEXs and MEV protection services",
  "keywords": [
    "solana",
    "trading",
    "trading-bot",
    "defi",
    "dex",
    "pump.fun",
    "pumpfun",
    "pumpswap",
    "raydium",
    "raydium-amm",
    "raydium-clmm",
    "raydium-cpmm",
    "raydium-launchpad",
    "bonk",
    "orca",
    "whirlpools",
    "meteora",
    "meteora-damm-v1",
    "meteora-damm-v2",
    "meteora-dlmm",
    "meteora-dbc",
    "moonit",
    "heaven",
    "sugar",
    "boop-fun",
    "jito",
    "nozomi",
    "astralane",
    "mev-protection"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/survivorff/solana_dex_node.git"
  },
  "homepage": "https://github.com/survivorff/solana_dex_node#readme",
  "bugs": {
    "url": "https://github.com/survivorff/solana_dex_node/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && chmod +x dist/cli.js || true",
    "start": "node dist/index.js",
    "precli": "npm run build",
    "cli": "node -r dotenv/config dist/cli.js",
    "api": "ts-node -r dotenv/config src/api-server.ts",
    "api:dev": "nodemon --watch src --exec ts-node -r dotenv/config src/api-server.ts",
    "test:api": "ts-node -r dotenv/config test-api-flow.ts"
  },
  "dependencies": {
    "@meteora-ag/cp-amm-sdk": "^1.1.1",
    "@meteora-ag/dlmm": "^1.7.3",
    "@meteora-ag/dynamic-amm-sdk": "^1.4.1",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.4.0",
    "@moonit/sdk": "^1.3.0",
    "@orca-so/whirlpools-sdk": "^0.16.0",
    "@pump-fun/pump-sdk": "^1.19.0",
    "@pump-fun/pump-swap-sdk": "^1.10.0",
    "@raydium-io/raydium-sdk-v2": "^0.2.23-alpha",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.0",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "cook-sdk": "^0.1.54",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "sugar-money": "^0.1.36"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "bin": {
    "solana-trade": "dist/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
