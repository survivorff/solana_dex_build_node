# API server port
API_PORT=3000

# Pairs cache TTL (milliseconds)
PAIRS_CACHE_TTL_MS=60000

# ============ Wallet Configuration ============
# Your wallet private key (base58 encoded)
# üîê KEEP THIS SECRET! Never commit to git!
PRIVATE_KEY=

# ============ MEV Protection (Optional) ============
# Jito Labs - No minimum tip required
JITO_UUID=

# Nozomi - Minimum tip: 0.001 SOL
NOZOMI_API_KEY=
NOZOMI_API_KEY_ANTIMEV=

# Astralane - Minimum tip: 0.00001 SOL
ASTRALANE_API_KEY=

# ============ PumpSwap Configuration ============
